<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fotballap">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.FindPlayerActivity"></activity>
        <activity android:name=".Activities.RaitingActivity" />
        <activity android:name=".Activities.MenuActivity" />
        <activity
            android:name=".Activities.dsffds"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_dsffds"
            android:theme="@style/FullscreenTheme" />
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.TournamentTableActivity" />
        <activity android:name=".Activities.AboutActivity" />
        <activity android:name=".Activities.PlayerActivity" />
    </application>

</manifest>